{
  "manifest_version": 2,
  "name": "Studon Autologin",
  "version": "1.4",
  "description": "Automatically start the login process for Studon. (Saves you !two! clicks)",
  "content_scripts": [
    {
      "matches": [
        "*://*.studon.fau.de/*"
      ],
      "js": [
        "login.js"
      ]
    }
  ],
  "icons": {
    "48": "icons/logo_48.png",
    "128": "icons/logo_128.png",
    "256": "icons/logo_256.png",
    "512": "icons/logo_512.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/logo_48.png",
      "128": "icons/logo_128.png",
      "256": "icons/logo_256.png",
      "512": "icons/logo_512.png"
    }
  }
}